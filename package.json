{
  "name": "generate-dotenv",
  "version": "1.2.1",
  "private": true,
  "description": "Generates a dotenv file within GitHub Actions.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "package": "ncc build src/main.ts --license licenses.txt",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iacobfred/generate-dotenv.git"
  },
  "keywords": [
    "actions",
    "node",
    "dotenv",
    "github"
  ],
  "author": "Jacob Fredericksen <jacob.t.fredericksen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iacobfred/generate-dotenv/issues"
  },
  "homepage": "https://github.com/iacobfred/generate-dotenv#readme",
  "dependencies": {
    "@actions/cache": "^3.0.4",
    "@actions/core": "^1.9.1",
    "dotenv": "^16.0.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.23.0",
    "husky": "^8.0.1",
    "jest": "^27.4.7",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.8.1",
    "typescript": "~4.6.4"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
